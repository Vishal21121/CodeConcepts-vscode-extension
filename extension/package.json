{
  "name": "vscodeextension",
  "displayName": ".",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscodeextension.deleteLang",
        "title": "Delete"
      },
      {
        "command": "vscodeextension.addLang",
        "title": "Add"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "conceptReviser",
          "title": "Concept Reviser",
          "icon": "assets/crosshair.svg"
        }
      ]
    },
    "views": {
      "conceptReviser": [
        {
          "id": "availableLanguages",
          "name": "Available Languages"
        },
        {
          "id": "choosenLanguage",
          "name": "Choosen Language"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "vscodeextension.deleteLang",
          "group": "inline"
        },
        {
          "command": "vscodeextension.addLang",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
